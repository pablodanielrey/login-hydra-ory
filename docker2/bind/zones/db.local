;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	dominio. root.dominio. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

hydra   A   192.168.0.3
consent A   192.168.0.3
client  A   192.168.0.3
usuarios	A	192.168.0.3
